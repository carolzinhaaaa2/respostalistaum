package Gato;

public class Gato {
  private String nome;
  private String raça;
  private int idade;
  
 public Gato(String n, String r, int i) {
	 this.nome= n;
	 this.raça= r;
	 this.idade= i;
  }
 public String getNome() {
	 return nome;
 }
 public String getRaça() {
	 return raça;
 }
 public String getIdade() {
	 return idade;
 }
 public void setNome() {
	 this.nome= nome;
 }
 public void setRaça() {
	 this.raça= raça;
 }
 public void setIdade() {
	 this.idade= idade;
 }
 public String miau() {
	 return "miau miau";
  } 
}
